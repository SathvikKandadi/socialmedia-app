{
  "expo": {
    "name": "ProveYourself",
    "slug": "py-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pylogo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pylogo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.proveyourself.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/pylogo.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/pylogo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7b9a41a9-73a2-4087-a253-48f4dc78718a"
      }
    }
  }
}
